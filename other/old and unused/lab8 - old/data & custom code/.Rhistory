zx <- x1[k1 < (100*(x1^2)*abs(y1)/l.max)]
zy <- y1[k1 < (100*(x1^2)*abs(y1)/l.max)]
box.area <- diff(range(t$x)) * diff(range(t$y))
eTile[i] <- sum(point.in.polygon(zx, zy, t$x, t$y)) * box.area *(l.max/gen.n)
}
return(eTile)
}
